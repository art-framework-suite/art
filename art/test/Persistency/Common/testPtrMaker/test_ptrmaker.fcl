physics:
{
  producers:
  {
    pm1:
    {
      module_type: IntVectorProducer
      nvalues: 16
    }
    pm2:
    {
      module_type: PtrMakerProducer
      input_label: pm1
    }
  }

  analyzers:
  {
    am1:
    {
      module_type: PtrMakerAnalyzer
      input_label: pm2
      nvalues: 16
    }
  }

   p1: [ pm1, pm2 ]
   e1: [ am1, out1 ]
}

outputs:
{
  out1:
  {
    module_type: RootOutput
    fileName: "outint.root"
  }
}

source:
{
  module_type: EmptyEvent
  # were in the global area before
  maxEvents: 1
}

process_name: DEVEL
