process_name: SAMMetadataW

source:
{
  module_type: EmptyEvent
  maxEvents: 10
  numberEventsInSubRun: 5
}

services.FileCatalogMetadata.checkSyntax: true

physics:
{
  producers:
  {
    m1:
    {
      module_type: IntProducer
      ivalue: 2
    }
  }

  analyzers:
  {
   a: { module_type: SAMMetadataTest }
  }

   p1: [ m1 ]
   e1: [ a, o1 ]

   trigger_paths: [ p1 ]
   end_paths: [ e1 ]
}

outputs:
{
  o1:
  {
    module_type: RootOutput
    fileName: "out.root"
  }
}
