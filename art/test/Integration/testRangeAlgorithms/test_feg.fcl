physics:
{
  producers:
  {
    pm1:
    {
      module_type: AssnsProducer1
    }
    pm2:
    {
      module_type: AssnsProducer2
      input_label: pm1
    }
  }

  analyzers:
  {
    am:
    {
      module_type: AssnsAnalyzerFEG
      input_label: pm2
    }
  }

   p1: [ pm1, pm2 ]
   e1: [ am, out1 ]
}

outputs:
{
  out1:
  {
    module_type: RootOutput
    fileName: "out.root"
  }
}

source:
{
  module_type: EmptyEvent
  # were in the global area before
  maxEvents: 3
}

process_name: DEVEL
