#include "messageDefaults.fcl"

services.message: @local::messageDefaults

physics: {
  producers: {
    unputter: {
      module_type: UnputtingProducer
    }
  }

  p1: [ unputter ]
  e1: [ o1 ]
}

outputs: {
  o1: {
    module_type: RootOutput
    fileName:  "unputting.root"
  }
}

source: {
  module_type: EmptyEvent
  maxEvents: 2
}

process_name: DEVEL
