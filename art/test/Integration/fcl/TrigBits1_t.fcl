process_name: PROD

services.scheduler.wantSummary: true

source: {
  module_type: EmptyEvent
  maxEvents: 10
}

physics: {
  producers: {
    m1a: {
      module_type: IntProducer
      ivalue: 1
    }
    m2a: {
      module_type: IntProducer
      ivalue: 1
    }
  }

  analyzers: {
    a1: {
      module_type: TestResultAnalyzer
      dump: true
      numbits: 2
    }
  }

  p1a: [ m1a ]
  p2a: [ m2a ]

  e1: [ a1 ]
  e2: [ testout1 ]
}

outputs: {
  testout1: {
    module_type: TestBitsOutput
    bitMask: 5
    SelectEvents: [ p2a ]
  }
}
