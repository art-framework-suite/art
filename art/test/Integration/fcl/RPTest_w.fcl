BEGIN_PROLOG
rp_modules: {
   results: {
      producers: {
         rpw: {
            plugin_type: RPTest
         }
         rpr: {
            plugin_type: RPTestReader
            intResultsLabel: @nil
            nResultsExpected: 1
         }
      }
      rpath: [rpw, rpr]
   }
}
END_PROLOG

process_name: DEVEL

source: {
  module_type: EmptyEvent
  maxEvents: 20
  numberEventsInSubRun: 5
  resetEventOnSubRun: false
}

physics: {
  e1: [o1, o2]
}

outputs: {
    o1: {
       module_type: RootOutput
       fileName: "out_r%r.root"
       @table::rp_modules
    }

   o2: {
      module_type: RootOutput
      fileName: "out_r%r_%#.root"
      maxEventsPerFile: 10
      @table::rp_modules
   }
}

outputs.o1.results.producers.rpr.intResultsLabel: "o1#rpw"
outputs.o2.results.producers.rpr.intResultsLabel: "o2#rpw"