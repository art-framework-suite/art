process_name: "SWRITE"

physics:
{
  producers:
  {
    mixProducer:
    {
      module_type: MixProducer
      errorOnFailureToPut: false
    }
  }

  p1: [ mixProducer ]
  e1: [ out ]
}

outputs:
{
  out:
  {
    module_type: RootOutput
    fileName: "mix.root"
  }
}

source:
{
  module_type: EmptyEvent
  numberEventsInRun: 500
  numberEventsInSubRun: 100
  maxEvents: 1000
}
