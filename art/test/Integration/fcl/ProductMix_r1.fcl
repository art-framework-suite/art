process_name: "SREAD"

physics:
{
  filters:
  {
    mixFilter:
    {
      module_type: MixFilterTest
      numSecondaries: 5
      mixProducerLabel: mixProducer
      fileNames: [ "../ProductMix_w.d/mix.root" ]
      readMode: sequential
    }
  }

  analyzers:
  {
    mixAnalyzer:
    {
      module_type: MixAnalyzer
      numSecondaries: 5
      mixFilterLabel: mixFilter
    }
  }

  p1: [ mixFilter ]
  e1: [ mixAnalyzer, out ]
}

outputs:
{
  out:
  {
    module_type: RootOutput
    fileName: "out.root"
  }
}

source:
{
  module_type: EmptyEvent
  maxEvents: 200
}
