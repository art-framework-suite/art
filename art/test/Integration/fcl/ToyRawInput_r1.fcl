services.scheduler.wantTracer: true

physics:
{
  producers:
  {
    rp1:
    {
      module_type: BareStringProducer
      value: "String2"
    }
  }
  analyzers:
  {
    wp1a:
    {
      module_type: BareStringAnalyzer
      input_label: wp1
      expected_value: "String1"
    }
    rp1a:
    {
      module_type: BareStringAnalyzer
      input_label: rp1
      expected_value: "String2"
    }
    a1:
    {
      module_type: ToyRawProductAnalyzer
      beginRun: false
      beginSubRun: false
    }
  }

  p: [ rp1 ]
