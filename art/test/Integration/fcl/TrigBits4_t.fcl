process_name: PROD

services.scheduler.wantSummary: true

source:
{
  module_type: EmptyEvent
  maxEvents: 10
}

physics:
{
  producers:
  {
    m1:
    {
      module_type: IntProducer
      ivalue: 1
    }
    m2:
    {
      module_type: IntProducer
      ivalue: 2
    }
    m3:
    {
      module_type: IntProducer
      ivalue: 3
    }
    m4:
    {
      module_type: IntProducer
      ivalue: 4
    }
  }

  analyzers:
  {
    a1:
    {
      module_type: TestResultAnalyzer
      dump: true
      numbits: 4
    }
  }

  p1: [ m1 ]
  p2: [ m2 ]
  p3: [ m3 ]
  p4: [ m4 ]

  e1: [ testout1 ]
  e2: [ a1 ]

  trigger_paths: [ p1, p2, p3, p4 ]
  end_paths: [ e1, e2 ]
}

outputs:
{
  testout1:
  {
    module_type: TestBitsOutput
    bitMask: 85
  }
}
