services: {
  scheduler: {
    wantSummary : true
  }

  TimeTracker: {
    printSummary: true
    dbOutput: {
      filename : "timeTracker.db"
      overwrite: true
    }
  }
}

physics: {

  producers: {
    prod: {
      module_type: TestTimeTrackerProducer
    }
  }

  filters: {
    filt: {
      module_type: TestTimeTrackerFilter
    }
  }

  analyzers: {
    mod1: {
      module_type: TestTimeTrackerAnalyzer
      SelectEvents: [p1]
    }

    mod2: {
      module_type: TestTimeTrackerAnalyzer
    }
  }

  p1: [prod, filt]
  e1: [mod1, mod2]
}

source: {
  module_type: EmptyEvent
  maxEvents: 1000
}

process_name: TimingServiceTest1
