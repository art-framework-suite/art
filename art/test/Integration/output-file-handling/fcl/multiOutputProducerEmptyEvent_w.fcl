source: {
  module_type: EmptyEvent
  maxEvents: 14
  firstRun: 1
  firstSubRun: 0
  numberEventsInRun: 10
  numberEventsInSubRun: 5
}

physics: {

  producers: {
    prod: { module_type: ToyProductProducer }
  }

  p1: [prod]
  e1: [o1, o2, o3, o4]
}

outputs: {

  o1: {
    module_type: RootOutput
    fileName : "out.root"
  }

  o2: {
    module_type: RootOutput
    fileName: "out_r%R.root"
    fileProperties: {
      maxRuns: 1
    }
  }

  o3: {
    module_type: RootOutput
    fileName: "out_r%r_sr%S.root"
    fileProperties: {
      maxSubRuns: 1
    }
  }

  o4: {
    module_type: RootOutput
    fileName : "out_r%r_sr%S_e%#.root"
    fileProperties: {
      maxEvents: 1
    }
  }

}

process_name: test1
