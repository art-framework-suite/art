source: {
  module_type: RootInput
}

physics: {

  e1: [o1, o2, o3, o4]
}

outputs: {

  o1: {
    module_type: RootOutput
    fileName : "out.root"
  }

  o2: {
    module_type: RootOutput
    fileName: "out_r%R.root"
    fileProperties: {
      maxRuns: 1
      granularity: Run
    }
  }

  o3: {
    module_type: RootOutput
    fileName: "out_r%r_sr%S.root"
    fileProperties: {
      maxSubRuns: 1
      granularity: SubRun
    }
  }

  o4: {
    module_type: RootOutput
    fileName : "out_r%r_sr%S_e%#.root"
    fileProperties: {
      maxEvents: 1
      granularity: Event
    }
  }

}

process_name: test1
