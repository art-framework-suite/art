#include "art/Framework/Art/@ART_MAIN_FUNC@.h"
#include "art/Framework/Art/detail/info_success.h"

#include "messagefacility/MessageLogger/MessageLogger.h"

#include <iostream>

int main( int argc, char* argv[] ) {
  int result = @ART_MAIN_FUNC@(argc,argv);
  // Make sure the message logger is shut down and any backlog cleared
  // so our message really is the last one put out.
  mf::EndMessageFacility();
  // End message.
  if (result == art::detail::info_success()) {
    return 0;
  }
  std::cout
    << "Art has completed and will exit with status "
    << result
    << "."
    << std::endl;
  return result;
}

