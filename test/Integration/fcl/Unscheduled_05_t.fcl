#include "messageDefaults.fcl"

process_name: "TEST"

services:
{
  scheduler:
  {
    allowUnscheduled: false
    #wantTracer: true
    Rethrow: [ "ProductNotFound" ]
  }
}

services.message: @local::messageDefaults

physics:
{
  producers:
  {
    one:
    {
      module_type: IntProducer
      ivalue: 1
    }
    two:
    {
      module_type: IntProducer
      ivalue: 2
    }
  }
  analyzers:
  {
    getOne:
    {
      module_type: IntTestAnalyzer
      input_label: "one"
      expected_value: 1
    }
    getTwo:
    {
      module_type: IntTestAnalyzer
      input_label: "two"
      expected_value: 2
    }
  }

  p: [ one, two ]
  e: [ getOne, getTwo ]

  trigger_paths: [ p ]
  end_paths: [ e ]
}

source:
{
  module_type: EmptyEvent
  maxEvents: 3
  timeBetweenEvents: 10
  firstTime: 1000000
}
