#include "messageDefaults.fcl"

process_name: TESTING

services:
{
  wantTracer: true
}

services.messageDefaults: @local::messageDefaults

source: { module_type: EmptyEvent maxEvents: 5 }
physics:
{
  producers:
  {
    m1:
    {
      module_type: IntVectorProducer
      nvalues: 7
     }
  }
  analyzers:
  {
    a1:
    {
      module_type: PtrListAnalyzer
      input_label: m1
      nvalues: 7
    }
  }
  reconstruction: [ m1 ]
  analysis: [ a1 ]
  trigger_paths: [ reconstruction ]
  end_paths:     [ analysis ]
}

outputs: { }
