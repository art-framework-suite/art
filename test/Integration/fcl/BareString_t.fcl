#include "messageDefaults.fcl"

process_name: DEVEL

services:
{
  message: @local::messageDefaults

  scheduler:
  {
    wantTracer: true
  }
}

physics:
{
  producers:
  {
    p1:
    {
      module_type: BareStringProducer
      value: Mummy
    }
  }

  t1: [ p1 ]
  trigger_paths: [ t1 ]
  e1: [ out1 ]
  end_paths: [ e1 ]
}

outputs:
{
  out1:
  {
    module_type: RootOutput
    fileName: "out.root"
  }
}

source:
{
  module_type: EmptyEvent
  maxEvents: 1
}
