process_name: DEVEL

source: {
  module_type: EmptyEvent
  maxEvents: 20
  numberEventsInSubRun: 5
  resetEventOnSubRun: false
}

physics: {
  e1: [ o1 ]
}

outputs: {
  o1: {
    module_type: RootOutput
    fileName: "out_%r.root"
    results: {
      producers: {
        rp1: {
          plugin_type: RPTest
        }
      }
      rpath: [ rp1 ]
    }
  }
}
