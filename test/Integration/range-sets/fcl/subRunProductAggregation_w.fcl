source: {
   module_type: EmptyEvent
   maxEvents: 14
   firstRun: 1
   firstSubRun: 0
   numberEventsInRun: 10
   numberEventsInSubRun: 5
}

physics: {

   producers: {
      prod: { module_type: SubRunProductProducer }
   }

   p1: [prod]
   e1: [o1, o2]
}

outputs: {

   o1: {
      module_type: RootOutput
      fileName: "out.root"
   }

   o2: {
      module_type: RootOutput
      fileName: "out_r%r_sr%s.root"
      fileSwitch: {
         force: true
         boundary: SubRun
      }
   }
}

process_name: test1
